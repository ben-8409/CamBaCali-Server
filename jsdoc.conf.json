{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": [ "app.js", "lib" ],
        "exclude": [ "bower_components", "node_modules", "routes" ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    }
}